# ==============================================
# CODERBOT V2 - CONFIGURAÇÕES DO AMBIENTE
# ==============================================

# Configurações do DeepSeek
DEEP_SEEK_API_KEY=your_deep_seek_api_key
DEEP_SEEK_API_URL=https://api.deepseek.com/v1

# Configurações do Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key

# Configuração do PocketBase
POCKETBASE_URL=http://127.0.0.1:8090
POCKETBASE_USER_EMAIL=your_email@example.com
POCKETBASE_USER_PASSWORD=your_password
POCKETBASE_ADMIN_EMAIL=admin@example.com
POCKETBASE_ADMIN_PASSWORD=admin_password

# Configurações de Providers de IA
OPEN_AI_API_KEY=your_openai_api_key
OPENAI_API_URL=https://api.openai.com/v1
CLAUDE_API_KEY=your_claude_api_key

# Outros
RAPIDAPI_KEY=your_rapidapi_key

# Configuração do Code Server (opcional)
CODE_SERVER_PASSWORD=your_code_server_password

# Ambiente de desenvolvimento
NODE_ENV=development
